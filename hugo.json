{
   "baseURL": "https://eoniku.github.io/",
   "languageCode": "ja-jp",
   "title": "のんのんだより",
   "theme": "hugo-heisei",
   "timeZone": "Asia/Tokyo",
   "relativeURLs": false,
   "canonifyURLs": true,
   "googleAnalytics": "G-XC19CPFQGX",
   "pagination": {
      "disableAliases": false,
      "pagerSize": 10,
      "path": "page"
   },
   "outputs": {
      "home": [
         "HTML",
         "RSS"
      ],
      "section": [
         "HTML",
         "RSS"
      ],
      "taxonomy": [
         "HTML",
         "RSS"
      ],
      "term": [
         "HTML",
         "RSS"
      ]
   },
   "params": {
      "description": "株スイングトレードとWarframeについて"
   },
   "markup": {
      "goldmark": {
         "renderer": {
            "unsafe": true,
            "hardWraps": true
         }
      }
   },
   "build": {
      "buildStats": {
         "enable": true
      },
      "cachebusters": [
         {
            "source": "assets/notwatching/hugo_stats\\.json",
            "target": "css"
         },
         {
            "source": "(postcss|tailwind)\\.config\\.js",
            "target": "css"
         }
      ]
   },
   "module": {
      "mounts": [
         {
            "source": "assets",
            "target": "assets"
         },
         {
            "disableWatch": true,
            "source": "hugo_stats.json",
            "target": "assets/notwatching/hugo_stats.json"
         }
      ]
   }
}